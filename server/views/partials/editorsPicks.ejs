<% editorsPicks.forEach(editorsPick => { %>
    <div class="col-12 col-lg-4">
    <div class="single-blog-post">
        <div class="post-thumb">
            <a href="#"><img src="<%= editorsPick.imgSrc %>" alt=""></a>
        </div>
        <div class="post-data">
            <a href="#" class="post-title">
                <h6><%= editorsPick.textContent %></h6>
            </a>
            <div class="post-meta">
                <div class="post-date"><a href="#"><%= editorsPick.time%></a></div>
            </div>
        </div>
    </div>
</div>
<%});%>


